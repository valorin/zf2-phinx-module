{
    "name": "valorin/zf2-phinx-module",
    "description": "Integrates the Phinx database migration tool into a ZF2 application.",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2", "phinx", "module"
    ],
    "homepage": "https://github.com/valorin/zf2-phinx-module",
    "authors": [
        {
            "name": "Stephen Rees-Carter",
            "homepage": "http://stephen.rees-carter.net/"
        }
    ],
    "autoload": {
        "psr-0": {
            "PhinxModule": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require": {
        "robmorgan/phinx": "~0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    }
}
